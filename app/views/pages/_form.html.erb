     <%= simple_form_for [ @user, @trip ] do |f| %>
        <%= f.input :name, placeholder: "Mars" %>
        <%= f.input :arrival_date, placeholder: "01.01.2021" %>
        <%= f.input :departure_date, placeholder: "30.06.2021" %>
        <%= f.input :cost_per:day, placeholder: "spacestar" %>
        <%= f.submit "Add", class: "btn btn-primary" %>
      <% end %>
